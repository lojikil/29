# simple, non-tail-recursive fib example
def fib n : int => int = if (<= n 1) then 1 else (+ (fib (- n 1)) (fib (- n 2)))

def main ac : int al : array of string => int = {

    when (!= ac 2) do {
        printf "fib [number]\\n"
        -1
    }

    val n : int = (atoi (get al 1))
    val t : int = (fib n)
    printf "(fib %d) == %d\\n" n t
    0
}
